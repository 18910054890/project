		<dependency>
			<groupId>cn.hutool</groupId>
			<artifactId>hutool-all</artifactId>
			<version>5.1.4</version>
		</dependency>
		<dependency>  
            <groupId>com.lyzd.om.app</groupId>  
            <artifactId>om-employee-sdk</artifactId> 
            <version>0.0.1-SNAPSHOT</version>  
        </dependency>

	</dependencies>
	
	<build>
     <plugins>
         <plugin>
             <groupId>org.springframework.boot</groupId>
             <artifactId>spring-boot-maven-plugin</artifactId>
             <configuration>
                    <!-- 没有该配置，devtools 不生效 -->
                    <fork>true</fork>
             </configuration>
         </plugin>
     </plugins>
  </build>
