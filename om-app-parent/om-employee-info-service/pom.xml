		<dependency>
			<groupId>org.mockito</groupId>
			<artifactId>mockito-junit-jupiter</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.lyzd.om</groupId>
			<artifactId>om-shared-model</artifactId>
		</dependency>
		<dependency>
			<groupId>com.lyzd.om</groupId>
			<artifactId>om-spring-common</artifactId>
		</dependency>
